{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "button-type": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-grid-column"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 107",
    "not and_ff <= 106",
    "not chrome <= 107",
    "not edge <= 107",
    "not firefox <= 107",
    "not ios_saf <= 16.1",
    "not opera <= 92",
    "not safari <= 16.1"
  ]
}